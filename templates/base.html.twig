    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}Low Production{% endblock %}</title>

    <meta name="description" content="{% block description %}Low Production est une association de production audiovisuelle œuvrant à la réalisation de documentaires engagés. Nous expérimentons des techniques de production à faible impact environnemental pour imaginer les métiers audiovisuels de demain.{% endblock %}">
    <meta name="author" content="Low Production">

    <!-- Balises OG -->
    <meta property="og:title" content="{{block('title')}}"/>
    <meta property="og:type" content="{% block og_type %}website{% endblock %}"/>
    <meta property="og:url" content="{{app.request.uri}}" />
    <meta property="og:image" itemprop="image" content="{% block og_img %}{{asset('web/img/og_image.png')}}{% endblock %}"/>
    <meta property="og:site_name" content="{{block('title')}}"/>
    <meta property="og:image:type" content="image/png">
    <meta property="og:image:width" content="300">
    <meta property="og:image:height" content="200">

